@model IEnumerable<IrsaWebStore.Models.ViewModel.Pages.PageVM>

@{ 
    string tempPageTitle = (string)TempData["PageTitle"];
}

@foreach (var item in Model) {
    if (tempPageTitle == item.Title)
    {
        <li class="active"><a href="/@item.Slug">@IrsaWebStore.Resources.GlobalRes.ResourceManager.GetString(item.Slug)</a></li>
    }
    else
    {
        <li><a href="/@item.Slug">@IrsaWebStore.Resources.GlobalRes.ResourceManager.GetString(item.Slug)</a></li>
    }

    @*<li><a href="/@item.Slug">@item.Title</a></li>*@
}

